            color: #f5f5f5;
            font-size: 16px;
        }
        .navbar .menu a:hover {
            color: #ff6600;
        }
        .content {
            text-align: center;
            padding: 50px 20px;
        }
        .profiles {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            padding: 20px;
        }
        .profile-box {
            background-color: #333;
            border: 1px solid #ff6600;
            border-radius: 8px;
            margin: 10px;
            padding: 20px;
            width: 200px;
            text-align: center;
            transition: transform 0.2s;
        }
        .profile-box:hover {
            transform: scale(1.05);
        }
        .profile-box h3 {
            font-size: 18px;
            color: #ff6600;
        }
        .profile-box p {
            font-size: 14px;
        }
        .footer {
            background-color: #333;
            color: #f5f5f5;
            text-align: center;
            padding: 40px 20px;
            position: relative;
            width: 100%;
        }
